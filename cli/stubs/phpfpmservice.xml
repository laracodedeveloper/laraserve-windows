<service>
  <id>laraserve_phpfpm</id>
  <name>laraserve_phpfpm</name>
  <description>Laraserve PHP-FPM</description>
  <executable>PHP_PATH\php-cgi.exe</executable>
  <arguments>-b 127.0.0.1:9001</arguments>
  <logpath>LARASERVE_HOME_PATH\Log\</logpath>
  <logmode>reset</logmode>
  <onfailure action="restart"/>
</service>
